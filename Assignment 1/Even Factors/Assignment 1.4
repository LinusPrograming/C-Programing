#include <stdio.h>
int main()
{
    int inum=0;

    printf("Enter a even number :");
    scanf("%d",&inum);

    EvenFactors(inum);

    return 0;
}
EvenFactors(int ivalue)
{
     int i=0;

     for(i=1;i<=ivalue;i++)
    {
        if(ivalue%i==0 && i%2==0)
        {
            printf("%d ",i);
        }
    }
}
