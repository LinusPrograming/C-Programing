#include <stdio.h>
int main()
{
    int ivalue=0;
    printf("Enter a number :\n");
    scanf("%d",&ivalue);
    if(ivalue>0)
    {
        printf("Positive number");
    }
    else if(ivalue<0)
    {
        printf("Negative number");
    }
    else
    {
        printf("Zero");
    }
    return 0;
}
