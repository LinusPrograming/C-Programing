#include <stdio.h>
void Accept(int ino)
{
    int inum=0;
    for(inum=1;inum<=ino;inum++)
    {
        if(inum%2==0)
        {
            printf("*");
        }
        else
        {
            printf("$");
        }
    }
}
int main()
{
    int ivalue=0;
    printf("Enter a number :\n");
    scanf("%d",&ivalue);
    Accept(ivalue);
    return 0;
}
