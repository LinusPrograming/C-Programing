#include <stdio.h>

int main()
{
    int iSno=0;
    int iEno=0;

    printf("Enter a starting number :");
    scanf("%d",&iSno);

    printf("Enter a ending number :");
    scanf("%d",&iEno);

    evenaddition(iSno,iEno);

    return 0;
}
int evenaddition(int iSnum,int iEnum)
{
    int ivalue1=0;
    int isum=0;

    for(ivalue1=iSnum;ivalue1<=iEnum;ivalue1++)
    {
        if(ivalue1%2==0)
        {
                isum=isum+ivalue1;
        }
    }
        printf("%d",isum);
}
