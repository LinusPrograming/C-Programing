#include <stdio.h>

int main()
{
   int ivalue1=0,ivalue2=0;

   printf("Enter starting point :");
   scanf("%d",&ivalue1);

   printf("Enter a ending point :");
   scanf("%d",&ivalue2);

   rangedisplayreverse(ivalue1,ivalue2);

    return 0;
}
void rangedisplayreverse(int istart,int iend)
{
    int inum=0;

    for(inum=iend;inum>=istart;inum--)
    {
        printf("%d",inum);
    }
    if(istart>iend)
    {
        printf("invalid number");
    }

}
