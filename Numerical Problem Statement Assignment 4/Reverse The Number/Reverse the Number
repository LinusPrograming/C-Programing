#include <stdio.h>'

int main()
{

    int originalnum=0;

    printf("Enter a number :");
    scanf("%d",&originalnum);

    PrintReverseNum(originalnum);
     return 0;
}
    int  PrintReverseNum(int inum)
    {
        int result=0;
        int reminder=0;


    while (inum!=0)
    {
        reminder=inum%10;

        result=result*10+reminder;

        inum/=10;
    }
   printf("%d",result);

}
