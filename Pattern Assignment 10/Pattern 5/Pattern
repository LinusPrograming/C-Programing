#include <stdio.h>

int main()
{
    int irow=0;
    int icol=0;

    printf("Enter a number of rows :");
    scanf("%d",&irow);
    printf("Enter a number of col :");
    scanf("%d",&icol);

    DisplayPattern(irow,icol);
    return 0;
}
int DisplayPattern(int ivalue1,int ivalue2)
{
    int ircount=0;
    int iccount=0;
    int inum=0;

    for(ircount=ivalue1;ircount>=1;ircount--)
    {
        for(iccount=ivalue2;iccount>inum;iccount--)
        {
                printf("%d\t",iccount);
        }
         inum--;

       ivalue2--;
        printf("\n");
    }
}
