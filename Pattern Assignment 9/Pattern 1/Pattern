#include <stdio.h>

int main()
{
    int irow=0;
    int icol=0;

    int inum1=0;
    int inum2=0;

    printf("Enter number of rows :");
    scanf("%d",&irow);

    printf("Enter number of col :");
    scanf("%d",&icol);

    for(inum1=1;inum1<=irow;inum1++)
    {
        for(inum2=1;inum2<=inum1;inum2++)
        {
            printf("*");
        }
         printf("\n");
    }

    return 0;
}





