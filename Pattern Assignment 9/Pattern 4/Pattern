#include <stdio.h>

int main()
{
    int irow=0;

    int inum1=0;
    int inum2=0;

    printf("Enter number of rows :");
    scanf("%d",&irow);

    for(inum1=1;inum1<=irow;inum1++)
    {
        for(inum2=1;inum2<=inum1*2-1;inum2++)
        {
            printf("%d",inum2);
        }
        printf("\n");
    }

    return 0;
}



